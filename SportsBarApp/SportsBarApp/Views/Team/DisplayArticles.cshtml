@using SportsBarApp.Models
@model TeamViewModel
@{
    ViewBag.Title = "Articles";
   
}
<h2 class="text-center">@ViewBag.Title</h2>
<div class="row">
    <div class="col-md-offset-2 col-md-8">
        <ul>
            @foreach (string art in Model.Articles)
            {
                <li><b>@Model.Team.ToUpper()</b>  <br /> @art</li>
            }

        </ul>
    </div>
</div>

